<% @drinks.each do |res| %>
	<div>
		<h2><%= res['name'] %><small> Rating: <%= res['rating'] %></small></h2>
		<h3> Ingredients: </h3>
		<ul>
			<% res['ingredients'].each do |el| %>
			<li><%= el['textPlain'] %></li>
			<% end %>
		</ul>
		<h3> Directions: <%= res['descriptionPlain'] %> </h3>

	</div>

<% end %>

