<% @drinks.each do |res| %>
	<div>
		<h2> Name: <%= res['name'] %><small> Rating: <%= res['rating'] %></small></h2>
		<h3> Ingredients: </h3>
		<% res['ingredients'].each do |el| %>
			<h3><%= el['textPlain'] %></h3>
		<% end %>
		<h3> Directions: <%= res['descriptionPlain'] %> </h3>
	</div>
<% end %>

